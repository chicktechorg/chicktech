<div class="col-md-12">
	<h2>All Events</h2>
  <div class="table-responsive">
    <table class="table">
      <thead>
        <tr>
          <th>Name</th>
          <th>City</th>
          <th>Start</th>
          <th>Leader</th>
          <th>Volunteers</th>

        </tr>
      </thead>
      <tbody>
        <% @events.each do |event| %>
        <tr>
          <td><%= link_to event.name, event_path(event) %></td>
          <td><%= event.city.name %></td>
          <td><%= event.start.strftime("%b %-d, %Y @ %-I:%M%P") %></td>
          <td><%= event.leader ?  link_to(event.leader.first_name, user_path(event.leader)) : "Take the lead" %></td>
          <td>cell</td>
        </tr>
       <% end %>
      </tbody>
    </table>

  </div>
</div>




