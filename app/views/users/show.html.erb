    <div id="wrapper">
    
      <!-- Page content -->
      <div id="page-content-wrapper">
        <div class="content-header">
        </div>
        <!-- Keep all page content within the page-content inset div! -->
        <div class="page-content inset">
          <div class="row">
            <div class="col-md-12">
            <div class="col-md-6">

              <h2 class='section-header well'><%= @user.first_name %>'s Commitments:</h2>
          <div class="job-info well">
            <% @user.unique_events.each do |event| %>
              <%= render event %>
              <% event.jobs.each do |job| %>
                <% if job.owned_by?(@user) %>
                <%= render job %>
                <% end %>
              <% end %>
            <% end %> 
          </div> 
        </div>



            </div>
            <div class="col-md-6 cities ">
              
             <%= render 'cities/dropdown', :cities => @cities %> 
             <div id="found-events">
             </div> 
            </div>
         
            </div>
          </div>
        </div>
      </div>
      
    </div>